# Application
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/scrape_agent?schema=public"

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Milvus Configuration
USE_PINECONE=false
MILVUS_ADDRESS=localhost:19530
MILVUS_USERNAME=
MILVUS_PASSWORD=
MILVUS_DATABASE=default
MILVUS_COLLECTION=job_embeddings
MILVUS_DIMENSION=1536
MILVUS_INDEX_TYPE=IVF_PQ
MILVUS_METRIC_TYPE=IP
# IVF parameters
MILVUS_NLIST=2048
MILVUS_NPROBE=64
# PQ parameters
MILVUS_M=8
MILVUS_NBITS=8
# GPU support
MILVUS_USE_GPU=false
MILVUS_GPU_ID=0

# Pinecone (Alternative Vector Store)
PINECONE_API_KEY=
PINECONE_ENVIRONMENT=
PINECONE_INDEX=job-embeddings

# AI Provider - OpenAI
OPENAI_API_KEY=
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_CHAT_MODEL=gpt-4-turbo-preview
OPENAI_MAX_RETRIES=3
OPENAI_TIMEOUT=60000

# AI Provider - Anthropic
ANTHROPIC_API_KEY=
ANTHROPIC_MODEL=claude-3-sonnet-20240229
ANTHROPIC_MAX_RETRIES=3
ANTHROPIC_TIMEOUT=60000

# AI Provider - Google Gemini
GEMINI_API_KEY=
GEMINI_MODEL=gemini-pro
GEMINI_EMBEDDING_MODEL=embedding-001

# Embedding Configuration
EMBEDDING_BATCH_SIZE=64
EMBEDDING_PROVIDER=openai
EMBEDDING_CACHE_TTL=604800

# Chunking Configuration
CHUNK_SIZE=512
CHUNK_OVERLAP=128
CHUNKING_STRATEGY=recursive

# Scraping Configuration
SCRAPER_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36
SCRAPER_TIMEOUT=30000
SCRAPER_MAX_RETRIES=3
SCRAPER_RESPECT_ROBOTS=true
SCRAPER_ENABLE_STEALTH=true

# Queue Configuration
QUEUE_SCRAPING_CONCURRENCY=5
QUEUE_INGESTION_CONCURRENCY=3
QUEUE_RETRY_ATTEMPTS=3
QUEUE_RETRY_DELAY=5000

# MCP Server
MCP_PORT=4000
MCP_HOST=0.0.0.0

# Metrics
PROMETHEUS_PORT=9090
METRICS_ENABLED=true

# Social Publishing
TWITTER_API_KEY=
TWITTER_API_SECRET=
TWITTER_ACCESS_TOKEN=
TWITTER_ACCESS_SECRET=
LINKEDIN_CLIENT_ID=
LINKEDIN_CLIENT_SECRET=
FACEBOOK_APP_ID=
FACEBOOK_APP_SECRET=

# Security
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100
CORS_ORIGIN=http://localhost:5173
JWT_SECRET=change-this-secret-in-production
