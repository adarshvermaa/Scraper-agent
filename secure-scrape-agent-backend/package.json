{
  "name": "secure-scrape-agent-backend",
  "version": "1.0.0",
  "description": "High-performance Scrape → Ingest → RAG → Multi-model Summarize → Publish system with Milvus vector store",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "milvus:setup": "ts-node scripts/setup-milvus.ts",
    "seed": "ts-node scripts/seed.ts",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:detached": "docker-compose -f docker-compose.dev.yml up --build -d",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build -d",
    "docker:prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "docker:down:dev": "docker-compose -f docker-compose.dev.yml down",
    "docker:down:prod": "docker-compose -f docker-compose.prod.yml down",
    "docker:build": "docker build -t scrape-agent-backend:latest .",
    "docker:clean": "docker-compose -f docker-compose.dev.yml down -v && docker-compose -f docker-compose.prod.yml down -v"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.0",
    "@bull-board/express": "^5.11.0",
    "@bull-board/nestjs": "^5.11.0",
    "@google/generative-ai": "^0.1.3",
    "@mozilla/readability": "^0.6.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/websockets": "^10.4.20",
    "@pinecone-database/pinecone": "^2.0.0",
    "@prisma/client": "^5.8.0",
    "@zilliz/milvus2-sdk-node": "^2.4.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "bullmq": "^5.1.0",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "eventsource-parser": "^1.1.1",
    "franc-min": "^6.2.0",
    "ioredis": "^5.3.2",
    "js-tiktoken": "^1.0.8",
    "jsdom": "^27.2.0",
    "langchain": "^0.1.7",
    "node-cron": "^4.2.1",
    "node-html-parser": "^6.1.12",
    "openai": "^4.24.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "playwright": "^1.40.1",
    "playwright-extra": "^4.3.6",
    "prom-client": "^15.1.0",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "reflect-metadata": "^0.2.2",
    "robots-parser": "^3.0.1",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "turndown": "^7.1.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bull": "^3.15.9",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prisma": "^5.8.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.103.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
